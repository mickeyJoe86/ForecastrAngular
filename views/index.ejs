<!Doctype html>
<html>
<head>
	<title>Forecastr | Get Your Forecast</title>
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u"
	 crossorigin="anonymous">
	<link rel="stylesheet" href="/dist/assets/css/all.css" />
</head>
<body ng-app="app">
	<shared-header></shared-header>

	<div id="main-containter" ng-controller="HomeComponentController as $homeCtrl">
		<div id="currently">
			<p>Temperature: {{ $homeCtrl.currently.temperature }}&deg</p>
			<p>Precip: {{ $homeCtrl.currently.precipProbability }}&#37</p>
			<p>Description: {{ $homeCtrl.currently.summary }}</p>
		</div>
	</div>

	<shared-footer></shared-footer>

	<!--main angular-->
	<script src="/angular.min.js"></script>
	<script src="/ui-bootstrap.js"></script>
	<script src="/dist/scripts.min.js"></script>

</body>
</html>
